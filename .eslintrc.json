{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
  },
  "extends": [
    "airbnb-base",
    "plugin:jasmine/recommended",
    "plugin:codeceptjs/recommended",
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "codeceptjs/no-pause-in-scenario": 0,
    "codeceptjs/no-skipped-tests": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "jasmine/no-disabled-tests": 0,
    "jasmine/no-focused-tests": 0,
    "linebreak-style": 0,
    "no-alert": 0,
    "no-await-in-loop": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-restricted-globals": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-underscore-dangle": 0
  },
  "plugins": ["jasmine", "codeceptjs"],
  "globals": { "PRODUCTION": true }
}
